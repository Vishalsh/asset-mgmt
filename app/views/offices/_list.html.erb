<script type="text/template" id="office_list">

	<%= render 'shared/navigation' %>

  <table class='table table-striped table-bordered'>
    <thead>
			<tr>
				<th>Name</th>
				<th></th>
				<th></th>
			</tr>
    </thead>
    <tbody>
			<@ _.each(offices, function(office) { @>
					<tr>
							<td><@= office.get('name') @></td>
							<td><a href="#offices/<@= office.get('_id')['$oid'] @>/edit" class="glyphicon glyphicon-pencil"></a></td>
							<td><a href="#offices/<@= office.get('_id')['$oid'] @>" class="glyphicon glyphicon-trash" data-confirm="Are you sure?"></a></td>
					</tr>
			<@ }); @>
    </tbody>
  </table>

</script>