<script type="text/template" id="admin_list">

	<%= render 'shared/navigation' %>

  <table class='table table-striped table-bordered'>
    <thead>
			<tr>
				<th>Ad id</th>
				<th>Name</th>
				<th></th>
				<th></th>
			</tr>
    </thead>
    <tbody>
			<@ _.each(admins, function(admin) { @>
					<tr>
							<td><@= admin.get('ad_id') @></td>
							<td><@= admin.get('name') @></td>
							<td><a href="#admins/<@= admin.get('_id')['$oid'] @>/edit" class="glyphicon glyphicon-pencil"></a></td>
							<td><a href="#admins/<@= admin.get('_id')['$oid'] @>" class="glyphicon glyphicon-trash delete-asset-type" data-confirm="Are you sure?"></a></td>
					</tr>
			<@ }); @>
    </tbody>
  </table>

</script>