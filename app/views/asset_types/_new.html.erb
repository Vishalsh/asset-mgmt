<script type="text/template" id="asset_type_new">

	<%= render 'asset_types/navigation' %>

<form class="asset-type-form">

	<div class='form-horizontal'>

		<div class="form-group">
			<lable class="control-label">Name</label>
			<input type="text" name="name" id="asset_type_name" class="form-control" />
			<div id="asset_type_name_error" class="error"></div>
		</div>

		<div class="form-group">
			<lable class="control-label">Image path</label>
			<input type="text" name="properties" id="asset_type_image_path" class="form-control" />
			<div id="asset_type_image_path_error" class="error"></div>
		</div>

		<div class="form-group">
			<lable class="control-label">Properties</label>
			<input type="text" name="properties" id="asset_type_properties" class="form-control" />
			<div id="asset_type_properties_error" class="error"></div>
		</div>

		<button type="submit" class="btn btn-primary">Submit</button>

	</div>
</form>







</script>