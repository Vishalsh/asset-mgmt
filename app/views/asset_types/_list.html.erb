<script type="text/template" id="asset_type_list">

	<%= render 'shared/navigation' %>

  <table class='table table-striped table-bordered'>
    <thead>
			<tr>
				<th>Name</th>
				<th>Properties</th>
				<th></th>
				<th></th>
			</tr>
    </thead>
    <tbody>
			<@ _.each(asset_types, function(asset_type) { @>
					<tr>
							<td><@= asset_type.get('name') @></td>
							<td><@= asset_type.get('properties') @></td>
							<td><a href="#asset_types/<@= asset_type.get('_id')['$oid'] @>/edit" class="glyphicon glyphicon-pencil"></a></td>
							<td><a href="#asset_types/<@= asset_type.get('_id')['$oid'] @>" class="glyphicon glyphicon-trash delete-asset-type" data-confirm="Are you sure?"></a></td>
					</tr>
			<@ }); @>
    </tbody>
  </table>

</script>